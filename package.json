{
    "name": "yukav3tssass",
    "version": "1.0.0",
    "description": "Un site de consultation de l'api open food facts, version avec TypeScript et Sass",
    "main": "index.js",
    "scripts": {
        "lint:md": "npx markdownlint **/*.md",
        "lint:scss": "npx stylelint scss/**/*.scss --formatter verbose",
        "lint:ts": "npx eslint scripts/*.ts",
        "lint:css": "npx stylelint styles/*.css --formatter verbose",
        "lint:html": "npx htmlhint **/*.html --ignore coverage/**/*.html, hint-report/**/*.html",
        "checkPrettier": "npx prettier --check index.html",
        "watch-tsWatch": "tsc --watch ./scripts/ts/index.ts --outDir ./scripts/js",
        "watch-tsc2": "tsc --watch ./scripts/ts/accordeon.ts --outDir ./scripts/js",
        "watch-tsc3": "tsc --watch ./scripts/ts/caracteristiques.ts --outDir ./scripts/js",
        "watch-tsc4": "tsc --watch ./scripts/ts/images.ts --outDir ./scripts/js",
        "watch-tsc5": "tsc --watch yukav3TsSassingredients.ts --outDir ./scripts/js",
        "watch-scssWatch": "sass --verbose --watch ./styles/sass/style.scss:./styles/css/style.css",
        "start": "concurrently \"npm:watch-*\"",
        "TscCompiler": "cd scripts/ts && tsc --out ../js/index.js ./accordeon.ts ./Produit.ts ./images.ts ./caracteristiques.ts ./ingredients.ts ./index.ts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/7Romain/yukav3TsSass.git"
    },
    "keywords": [
        "foodfact",
        "yuka",
        "aliment",
        "nutriments",
        "nutriscore",
        "ecoscore",
        "nourriture"
    ],
    "author": "Romain.7",
    "license": "CC0-1.0",
    "devDependencies": {
        "@hint/configuration-web-recommended": "^8.2.13",
        "@hint/hint-typescript-config": "^2.5.9",
        "@testing-library/dom": "^8.16.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@types/node": "^18.8.3",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.31.0",
        "autoprefixer": "^10.4.7",
        "babel-jest": "^28.1.3",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-recommended": "^4.1.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-compat": "^4.0.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.6.0",
        "eslint-plugin-n": "^15.2.4",
        "eslint-plugin-promise": "^6.0.0",
        "hint": "^7.1.0",
        "htmlhint": "^1.1.4",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "jest-html-reporters": "^3.0.10",
        "jsdoc": "^3.6.10",
        "jsdom": "^20.0.0",
        "markdownlint": "^0.26.2",
        "markdownlint-cli": "^0.32.0",
        "postcss": "^8.4.14",
        "postcss-cli": "^10.0.0",
        "postcss-sorting": "^7.0.1",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "snyk": "^1.1012.0",
        "sonarlint": "^1.1.0",
        "source-map-support": "^0.5.21",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-recommended": "^9.0.0",
        "stylelint-config-recommended-scss": "^7.0.0",
        "stylelint-scss": "^4.3.0",
        "typescript": "^4.8.4",
        "vscode-webhint": "^2.1.0"
    },
    "bugs": {
        "url": "https://github.com/7Romain/yukav3TsSass/issues"
    },
    "homepage": "https://github.com/7Romain/yukav3TsSass#readme",
    "dependencies": {
        "@types/express": "^4.17.14",
        "axios": "^1.0.0",
        "node-fetch": "^3.2.10",
        "sass": "^1.55.0"
    }
}
